pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Checkout from Jenkinsfile'
            }
        }
        stage('Build') {
            steps {
                echo 'Build from Jenkinsfile'
            }
        }
        stage('Test') {
            steps {
                echo 'Test from Jenkinsfile'
            }
        }
    }
}
